{
  "name": "upcloudltd/upcloud-php-api",
  "description": "UpCloud PHP API",
  "keywords": ["php", "sdk", "api", "upcloud"],
  "homepage": "https://github.com/UpCloudLtd/upcloud-php-api",
  "license": "MIT",
  "authors": [
    {
      "name": "UpCloud",
      "homepage": "https://www.upcloud.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^3.2",
    "friendsofphp/php-cs-fixer": "^2.16.0"
  },
  "autoload": {
    "psr-4": { "Upcloud\\ApiClient\\": "lib/" }
  },
  "autoload-dev": {
    "psr-4": { "Upcloud\\ApiClient\\": "test/" }
  },
  "minimum-stability": "stable"
}
